node{
   stage('SCM Checkout'){
     git 'https://github.com/kaustubh0228/GridJenkinsEx.git'
   }
   stage('SonarQube analysis') {
    withSonarQubeEnv(credentialsId: 'f69ded95-53c5-4916-adf3-e42abe1036cf', installationName: 'Sonar') { // You can override the credential to be used
      sh 'mvn org.sonarsource.scanner.maven:sonar-maven-plugin:3.6.0.1398:sonar'
    }
  }
}